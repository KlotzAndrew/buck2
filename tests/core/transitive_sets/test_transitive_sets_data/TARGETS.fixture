load(":defs.bzl", "optional_args", "optional_json_args", "test", "test_duplication")

test(name = "foo1")

test(name = "foo2")

test(
    name = "foo",
    deps = [
        ":foo1",
        ":foo2",
    ],
)

test(
    name = "bar",
    deps = [
        ":foo",
        ":foo1",
    ],
)

test_duplication(
    name = "test_duplication",
)

optional_args(name = "optional_args1", has_artifact = True)

optional_args(name = "optional_args2", has_artifact = False)

optional_args(
    name = "combined_optional_args",
    deps = [
        ":optional_args1",
        ":optional_args2",
    ],
    has_artifact = True,
)

optional_json_args(name = "optional_json_args1", has_artifact = True)

optional_json_args(name = "optional_json_args2", has_artifact = False)

optional_json_args(
    name = "combined_optional_json_args",
    deps = [
        ":optional_json_args1",
        ":optional_json_args2",
    ],
    has_artifact = True,
)
